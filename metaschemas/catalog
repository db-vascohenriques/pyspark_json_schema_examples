  "$id": "https://raw.githubusercontent.com/db-vascohenriques/pyspark_json_schema_examples/master/metaschemas/catalog",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "$id": {
      "type": "string",
      "format": "uri"
    },
    "$schema": {
      "type": "string",
      "format": "uri"
    },
    "type": {
      "const": "object"
    },
    "title": {
      "description": "The name of the catalog to be created",
      "type": "string"
    },
    "description": {
      "description": "The description for the cataloga",
      "type": "string"
    },
    "objectType": {
      "const": "catalog"
    }
  },
  "required": [
    "$id",
    "$schema",
    "title",
    "objectType"
  ]
}
