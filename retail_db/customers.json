{
  "$id": "https://github.com/db-vascohenriques/pyspark_json_schema_examples/raw/master/retail_db/customers.json",
  "$schema": "http://json-schema.org/schema",
  "title": "retail_db.customers",
  "description": "Customer dimension for sales",
  "properties": {
    "customer_id": {
      "type": "string",
      "description": "Primary key of customer dimension",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "primary_address": {
      "type": "string",
      "description": "Default Ship-To address for customer",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "email": {
      "type": "string",
      "description": "Redactable email address for customer",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "gender": {
      "type": "string",
      "description": "M or Male, F or Female, NB or Non-Binary, O or Other",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "birthdate": {
      "type": "object",
      "description": "Customer Date of Birth",
      "objectType": "column",
      "sparkSQLType": "date"
    },
    "sincedate": {
      "type": "object",
      "description": "Customer Date of account creation",
      "objectType": "column",
      "sparkSQLType": "date"
    },
    "attributes": {
      "type": "object",
      "description": "Infinite text-based customer attributes",
      "objectType": "column",
      "sparkSQLType": "map",
      "properties": {
        "keys": {
          "type": "string",
          "objectType": "nestedField",
          "sparkSQLType": "string"
        },
        "values": {
          "type": "string",
          "objectType": "nestedField",
          "sparkSQLType": "string"
        }
      }
    },
    "metrics": {
      "type": "object",
      "description": "Infinite text-based customer attributes",
      "objectType": "column",
      "sparkSQLType": "map",
      "properties": {
        "keys": {
          "type": "string",
          "objectType": "nestedField",
          "sparkSQLType": "string"
        },
        "values": {
          "type": "number",
          "objectType": "nestedField",
          "sparkSQLType": "double"
        }
      }
    }
  },
  "required": ["customer_id"]
}
