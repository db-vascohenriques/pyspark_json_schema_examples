{
  "$id": "https://github.com/db-vascohenriques/pyspark_json_schema_examples/raw/master/retail_db/outlets.json",
  "$schema": "https://raw.githubusercontent.com/db-vascohenriques/pyspark_json_schema_examples/master/metaschemas/tabview",
  "title": "hive_metastore.retail_db.outlets",
  "description": "Outlet Dimension for stores, distribution centers and warehouses",
  "type": "object",
  "objectType": "table",
  "properties": {
    "outlet_id": {
      "type": "string",
      "description": "Primary key of outlet dimension",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "description": {
      "type": "string",
      "description": "Plain text outlet description",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "type": {
      "type": "string",
      "description": "Outlet type, e.g. store, dc, warehouse",
      "objectType": "column",
      "sparkSQLType": "string"
    },
    "latitude": {
      "type": "number",
      "description": "Location latitude",
      "objectType": "column",
      "sparkSQLType": "double"
    },
    "longitude": {
      "type": "number",
      "description": "Location longitude",
      "objectType": "column",
      "sparkSQLType": "double"
    },
    "personnel": {
      "type": "integer",
      "description": "Total number of employees",
      "objectType": "column",
      "sparkSQLType": "int"
    },
    "customer_capacity": {
      "type": "integer",
      "description": "Maximum customers allowed in",
      "objectType": "column",
      "sparkSQLType": "int"
    },
    "holding_capacity": {
      "type": "integer",
      "description": "Maximum item count allowed",
      "objectType": "column",
      "sparkSQLType": "int"
    }
  },
  "required": ["outlet_id"]
}
